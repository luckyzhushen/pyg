.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cates {
  display: flex;
}
.index_cates navigator {
  padding: 20px;
  flex: 1;
}
.index_cates navigator image {
  width: 100%;
}
.index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_list {
  /*清除浮动*/
  overflow: hidden;
}
.index_floor .floor_group .floor_list .floor_list_list {
  float: left;
  width: 33.33%;
  /*后四个超链接*/
  /* 2 3 超链接加下边框*/
}
.index_floor .floor_group .floor_list .floor_list_list:nth-last-child(-n+4) {
  /*原图的宽高 232*386*/
  /*第一张的高height: 33.33vw * 386 /232;*/
  height: 27.72711207vw;
  /*加左边框*/
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list .floor_list_list:nth-child(2),
.index_floor .floor_group .floor_list .floor_list_list:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list .floor_list_list image {
  width: 100%;
  height: 100%;
}
